@inherits FormNotesBase

<EditForm id="@FormId" Model="@Model" OnValidSubmit="HandleFormSubmitted">
	<FluentValidationValidator Validator="ModelValidator"></FluentValidationValidator>
	<MudGrid Spacing="2" Class="mb-3">
		<MudItem xs="12">
			<MudTextField Variant="Variant.Outlined" Label="Workout Notes (optional)" Lines="6" FullWidth="true" @bind-Value="@Model.Notes" />
		</MudItem>
	</MudGrid>
</EditForm>

