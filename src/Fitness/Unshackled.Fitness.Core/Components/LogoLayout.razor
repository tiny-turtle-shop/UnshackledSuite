		.AddClass(Class)
		.Build();

	protected string SvgWidth => string.IsNullOrEmpty(Width) ? "100%" : Width;

	protected string SvgHeight => string.IsNullOrEmpty(Height) ? "100%" : Height;

	protected void HandleClick()
	{
		if (!string.IsNullOrEmpty(Href))
		{
			NavManager.NavigateTo(Href);
		}
	}

}
